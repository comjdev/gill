<main class="contact-page">
	<div class="container mx-auto px-4 py-12">
		<div class="max-w-7xl mx-auto">
			<!-- Two Column Layout: Content and Form -->
			<div class="grid grid-cols-1 lg:grid-cols-2 gap-12 mb-16">
				<!-- Content Column -->
				<div class="space-y-8">
					<!-- Page Content -->
					<div class="bg-white p-8">
						<div class="prose prose-lg max-w-none text-gray-500">
							<%- page.content %>
						</div>
					</div>
				</div>

				<!-- Form Column -->
				<div class="bg-white px-2 py-4">
					<div class="w-full">
						<iframe
							id="sn-form-puftr"
							src="https://app.studioninja.co/contactform/parser/0a800fc9-7801-1bf2-8178-1a1deab31908/0a800fc8-7801-1f4a-8178-1abb80313915"
							height="616"
							scrolling="no"
							style="width: 100%; border: 0"
						></iframe>
					</div>
				</div>
			</div>
		</div>
	</div>
</main>

<!-- Testimonial -->
<%- include('_partials/testimonial', { index: 1 }) %>

<aside class="container mx-auto px-4 py-12">
	<div class="max-w-7xl mx-auto">
		<h2
			class="text-2xl font-light leading-none text-gray-900 md:text-3xl lg:text-4xl my-4 text-center"
		>
			Melbourne Lifestyle Photographer
		</h2>
		<div
			class="grid gap-4 grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 mt-8 lg:mt-12 xl:mt-14 lg:gap-8 xl:gap-12 items-stretch"
		>
			<!-- prettier-ignore -->
			<% Object.keys(site.data.homepage.services).forEach(function(serviceKey) {
	const service = site.data.homepage.services[serviceKey]; %> <%-
	partial('_partials/service-card', { image: service.image, title:
	service.title, description: service.description, link: service.link,
	buttonText: service.title }) %> <% }); %>
		</div>
	</div>
</aside>
