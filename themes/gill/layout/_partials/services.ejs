<% site.pages.filter(page => page.layout ===
'service').forEach(function(service, index) { %>
<div class="service">
	<h2>
		<a href="<%= service.url %>"
			><%= service.title.replace(/\s+in\s+Melbourne/i, '') %></a
		>
	</h2>
	<p><%= service.excerpt %></p>
	<p>
		<a
			href="<%= service.url %>"
			class="inline-flex items-center px-4 py-2 text-sm font-light text-white bg-primary-700 hover:bg-primary-800 rounded-md"
			>Read more</a
		>
	</p>
</div>
<% }) %>
