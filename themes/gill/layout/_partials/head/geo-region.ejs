<meta name="geo.region" content="AU-VIC" />
<meta
	name="geo.placename"
	content="<%= page.layout === 'suburb' ? page.suburb : config.geolocation.suburb %>"
/>
<meta
	name="geo.position"
	content="<%= page.layout === 'suburb' ? page.latitude : config.geolocation.latitude %>;<%= page.layout === 'suburb' ? page.longitude : config.geolocation.longitude %>"
/>
<meta
	name="ICBM"
	content="<%= page.layout === 'suburb' ? page.latitude : config.geolocation.latitude %>, <%= page.layout === 'suburb' ? page.longitude : config.geolocation.longitude %>"
/>
