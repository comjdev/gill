<!-- What to expect -->
<section class="bg-white">
	<div class="py-8 px-4 mx-auto max-w-screen-xl sm:py-16 lg:px-6">
		<div class="max-w-screen-md mb-12">
			<h1 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900">
				<%= service.what_to_expect.title %>
			</h1>
			<h3 class="mb-4 text-2xl tracking-tight font-extrabold text-gray-900">
				<%= service.what_to_expect.subtitle %>
			</h3>
			<!-- prettier-ignore -->
			<% service.what_to_expect.copy.forEach(function(paragraph) { %>
			<p class="mb-8 font-light text-gray-900 sm:text-xl"><%= paragraph %></p>
			<% }); %>

			<a
				href="/book-lifestyle-photographer-in-melbourne/"
				class="inline-block px-8 py-4 mt-6 text-[10px] font-bold uppercase tracking-[0.25em] text-white bg-primary-800 no-underline border-none cursor-pointer transition-all duration-700 ease-in-out overflow-hidden relative z-[1] hover:bg-primary-500 transform translate-y-4 group-hover:translate-y-0"
				data-cursor="cursor-link"
			>
				Get in touch
			</a>
		</div>
	</div>
</section>
