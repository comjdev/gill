<!-- prettier-ignore -->
<main class="bg-white">

	<div class="py-8 px-4 mx-auto max-w-screen-xl sm:py-16 lg:px-6">
        <div class="mx-auto max-w-screen-lg text-center">
            <h2 class="mb-2 text-4xl tracking-tight font-extrabold text-gray-900 "><%= page.title %></h2>
            <p class="mb-8 text-gray-900 lg:text-lg "><%= page.subtitle %></p>
        </div>
	</div>

<% 
// Get all service data files
var serviceData = {
  'Family': site.data.family,
  'Maternity': site.data.maternity,
  'Newborn': site.data.newborn,
  'Wedding': site.data.wedding
};

// Loop through each service and display its FAQs
Object.keys(serviceData).forEach(function(serviceName) {
  var service = serviceData[serviceName];
  if (service && service.faqs && service.faqs.length > 0) {
%>
<!-- prettier-ignore -->
<%- partial('_partials/faqs', { service: serviceName, title: serviceName + ' Photography FAQs', faqs: service.faqs }) %>

<% 
  }
}); 
%>

</main>
